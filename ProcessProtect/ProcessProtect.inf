;
; ProcessProtect.inf
;

[Version]
Signature="$WINDOWS NT$"
Class=Sample ; TODO: edit Class
ClassGuid={78A1C341-4539-11d3-B88D-00C04FAD5171} ; TODO: edit ClassGuid
Provider=%ManufacturerName%
CatalogFile=ProcessProtect.cat
DriverVer= ; TODO: set DriverVer in stampinf property pages
PnpLockDown = 1

[DestinationDirs]
DefaultDestDir = 12
ProcessProtect_Device_CoInstaller_CopyFiles = 11

; ================= Class section =====================

[ClassInstall32]
Addreg=SampleClassReg

[SampleClassReg]
HKR,,,0,%ClassName%
HKR,,Icon,,-5

[SourceDisksNames]
1 = %DiskName%,,,""

[SourceDisksFiles]
ProcessProtect.sys  = 1,,
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll=1 ; make sure the number matches with SourceDisksNames

;*****************************************
; Install Section
;*****************************************

[Manufacturer]
%ManufacturerName%=Standard,NT$ARCH$

[Standard.NT$ARCH$]
%ProcessProtect.DeviceDesc%=ProcessProtect_Device, Root\ProcessProtect ; TODO: edit hw-id

[ProcessProtect_Device.NT]
CopyFiles=Drivers_Dir

[Drivers_Dir]
ProcessProtect.sys

;-------------- Service installation
[ProcessProtect_Device.NT.Services]
AddService = ProcessProtect,%SPSVCINST_ASSOCSERVICE%, ProcessProtect_Service_Inst

; -------------- ProcessProtect driver install sections
[ProcessProtect_Service_Inst]
DisplayName    = %ProcessProtect.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\ProcessProtect.sys

;
;--- ProcessProtect_Device Coinstaller installation ------
;

[ProcessProtect_Device.NT.CoInstallers]
AddReg=ProcessProtect_Device_CoInstaller_AddReg
CopyFiles=ProcessProtect_Device_CoInstaller_CopyFiles

[ProcessProtect_Device_CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll,WdfCoInstaller"

[ProcessProtect_Device_CoInstaller_CopyFiles]
WdfCoInstaller$KMDFCOINSTALLERVERSION$.dll

[ProcessProtect_Device.NT.Wdf]
KmdfService =  ProcessProtect, ProcessProtect_wdfsect
[ProcessProtect_wdfsect]
KmdfLibraryVersion = $KMDFVERSION$

[Strings]
SPSVCINST_ASSOCSERVICE= 0x00000002
ManufacturerName="<Your manufacturer name>" ;TODO: Replace with your manufacturer name
ClassName="Samples" ; TODO: edit ClassName
DiskName = "ProcessProtect Installation Disk"
ProcessProtect.DeviceDesc = "ProcessProtect Device"
ProcessProtect.SVCDESC = "ProcessProtect Service"
